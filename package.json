{
  "name": "vscode-fast-folder-structure",
  "displayName": "Flutter Redux Packages",
  "description": "Create New Redux Package for easy reusability",
  "repository": {
    "type": "git",
    "url": "https://github.com/Huuums/vscode-folder-templates"
  },
  "keywords": [
    "Flutter Redux",
    "Flutter Packages",
    "Flutter Redux Packages",
    "Flutter Redux Folders",
    "Create Flutter Redux Directories"
  ],
  "version": "4.4.0",
  "publisher": "EvgeniyNikiforov",
  "engines": {
    "vscode": "^1.37.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:FT.createReduxStructure",
    "onCommand:FT.createBlocWithWidgetStructure"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "FT.createReduxStructure",
        "title": "Create New Flutter Redux Package"
      },
      {
        "command": "FT.createBlocWithWidgetStructure",
        "title": "Create Bloc with Widget"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "FT.createReduxStructure",
          "group": "1_workspace@500"
        },
        {
          "when": "explorerResourceIsFolder",
          "command": "FT.createBlocWithWidgetStructure",
          "group": "1_workspace@500"
        }
      ]
    },
    "configuration": {
      "title": "Flutter Redux Packages",
      "properties": {
        "folderTemplates.structures": {
          "type": "array",
          "scope": "window",
          "default": [],
          "description": "Configure as many structures as you would like."
        },
        "folderTemplates.fileTemplates": {
          "type": "object",
          "scope": "window",
          "default": {}
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "package": "yarn run compile && vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/chai-fs": "^2.0.2",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/pluralize": "^0.0.29",
    "@types/rimraf": "^3.0.0",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.3",
    "@types/vscode": "^1.37.0",
    "all-contributors-cli": "^6.20.0",
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0",
    "glob": "^7.1.4",
    "mocha": "^8.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.0",
    "sinon-chai": "^3.5.0",
    "tslint": "^6.0.0",
    "typescript": "^4.1.2",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "@types/html-tags": "^3.0.1",
    "change-case": "^4.1.2",
    "html-tags": "^3.1.0",
    "open": "^7.3.0",
    "pluralize": "^8.0.0",
    "x-match-expression": "^0.2.1"
  }
}
